<div id="<%= dom_id report %>"class= "account-info-container">
  <p>
    <strong>Title:</strong>
    <%= report.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= report.content %>
  </p>

  <p>
    <strong>User:</strong>
    <%= report.user.email %>
  </p>
  <br>
  <%= link_to "Download CSV", download_csv_report_path(report, format: :csv), class: "btn btn-secondary" %>

</div>
